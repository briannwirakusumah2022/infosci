def setup():
    size(500, 500)
    background(255)
    
def draw():
    fill(0) 
    ellipse(100, 100, 100, 100)
    fill(0)
    ellipse(400, 100, 100, 100)
    fill(0)
    ellipse(250, 400, 100, 100)
    fill(0)
    
    stroke(0)
    strokeWeight (10)
    fill (255)
    triangle(250, 50, 100, 300, 400, 300)
    
    stroke(255)
    fill (255)
    triangle(100, 100, 400, 100, 250, 400)
    
    
    
